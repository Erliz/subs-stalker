{
  "name": "subs-stalker",
  "version": "0.3.0",
  "description": "Watch for video files and download subtitles",
  "main": "src/index.js",
  "scripts": {
    "test": "mocha test/index.js --compilers js:babel-core/register -w",
    "test:single": "babel-node node_modules/mocha/bin/_mocha test/index.js",
    "check-coverage": "babel-node node_modules/babel-istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha",
    "report-coverage": "cat ./coverage/lcov.info | codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Erliz/subs-stalker.git"
  },
  "keywords": [
    "subtitles",
    "anime"
  ],
  "author": "Stanislav Vetlovskiy",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Erliz/subs-stalker/issues"
  },
  "homepage": "https://github.com/Erliz/subs-stalker#readme",
  "dependencies": {
    "babel-core": "6.7.7",
    "babel-preset-es2015": "6.6.0",
    "bluebird": "3.3.5",
    "body-parser": "1.15.0",
    "chokidar": "1.4.3",
    "commander": "2.9.0",
    "content-disposition": "0.5.1",
    "eventemitter2": "1.0.1",
    "express": "4.13.4",
    "pushbullet": "1.4.3",
    "request": "2.72.0",
    "sqlite3": "3.1.3",
    "winston": "2.2.0"
  },
  "devDependencies": {
    "babel-cli": "6.7.7",
    "babel-istanbul": "0.8.0",
    "chai": "3.5.0",
    "codecov": "1.0.1",
    "mocha": "2.4.5",
    "mock-fs": "3.9.0",
    "nock": "8.0.0",
    "sinon": "1.17.3",
    "superagent": "1.8.3"
  }
}
